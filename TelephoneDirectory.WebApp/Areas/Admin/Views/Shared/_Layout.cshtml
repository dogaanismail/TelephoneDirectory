@using Microsoft.AspNet.Identity;
<!DOCTYPE html>
<html ng-app="telephoneDirectory">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>TelephoneDirectory Admin Panel| Dashboard</title>
    @*@Styles.Render("~/Content/AdminLTEcss")*@

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <!-- Ionicons -->
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE//dist/css/AdminLTE.css">
    <link href="~/Areas/Admin/Content/AdminLTE/plugins/colorpicker/bootstrap-colorpicker.css" rel="stylesheet" />

    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE//dist/css/skins/_all-skins.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/iCheck/flat/blue.css">
    <!-- Morris chart -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/morris/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/datepicker/datepicker3.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/daterangepicker/daterangepicker-bs3.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="~/Areas/Admin/Content/AdminLTE/plugins/datatables/dataTables.bootstrap.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>

<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/Admin/Dashboard/Index" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">Admin Panel</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Admin Panel</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                @*<img src="@HttpContext.Current.Session["PhotoPath"].ToString()" err-src="@HttpContext.Current.Session["PhotoPath"].ToString()" class="user-image" alt="User Image">*@
                                <span class="hidden-xs">  @User.Identity.GetUserName()</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    @*<img src="@HttpContext.Current.Session["PhotoPath"].ToString()" err-src="/FileUpload/UserProfile/User.jpg" class="img-circle" alt="User Image">*@
                                    <p>
                                        @User.Identity.GetUserName()
                                        <small>Member since @*@HttpContext.Current.Session["InsertDate"]*@</small>
                                    </p>
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="/Admin/User/Index?ID=@User.Identity.GetUserId()" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="/Admin/Account/LogOff" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        @*<img src="@HttpContext.Current.Session["PhotoPath"].ToString()" err-src="@HttpContext.Current.Session["PhotoPath"].ToString()" style="height:50px;" class="img-circle" alt="User Image">*@
                    </div>
                    <div class="pull-left info">
                        <p> @User.Identity.GetUserName()</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>

                <div ng-controller="MenuCtrl">

                    <ul class="sidebar-menu">
                        <li class="header">MAIN NAVIGATION</li>

                        <li ng-class="{'treeview': item.PageUrl=='#'}" ng-repeat="item in lstParentMenu | orderBy:'DisplayOrder'">
                            <a href="~/{{item.PageUrl}}">
                                <i class="fa {{item.PageIcon}}"></i>
                                <span>{{item.ModuleName}}</span>
                                <i ng-if="item.PageUrl=='#'" class="fa fa-angle-left pull-right"></i>
                            </a>
                            <ul class="treeview-menu" ng-repeat="obj in item.Submenu | orderBy:'DisplayOrder'">
                                <li>
                                    <a href="/{{obj.PageUrl}}">
                                        <i class="fa {{obj.PageIcon}}"></i>
                                        {{obj.ModuleName}}
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li><a href="/Admin/Account/ChangePassword"><i class="fa fa-list-alt"></i>Change Password</a></li>
                    </ul>

                </div>

            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->

        @RenderBody()

        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; @DateTime.Now.Year</strong> All rights
            reserved.
        </footer>

        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.0 -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/jQuery/jQuery-2.2.0.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/jQueryUI/jquery-ui.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.5 -->
    <script src="~/Areas/Admin/Content/AdminLTE/bootstrap/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/raphael/raphael-min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>*@
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/morris/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/moment/min/moment.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>*@
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/datepicker/bootstrap-datepicker.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Areas/Admin/Content/AdminLTE/dist/js/app.min.js"></script>
    <!-- ChartJS 1.0.1 -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/chartjs/Chart.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="~/Content/AdminLTE/dist/js/pages/dashboard2.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    <script src="~/Areas/Admin/Content/AdminLTE/dist/js/demo.js"></script>
    <!-- DataTables -->
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/datatables/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/controller/app.js"></script>
    <script src="~/Scripts/controller/controller.js"></script>
    <script src="~/Areas/Admin/Content/AdminLTE/plugins/underscore/underscore-min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
